    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.1.3",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "eslint": "^7.20.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-joi-validation": "^5.0.0",
    "http-status-codes": "^2.1.4",
    "jest": "^26.6.3",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
