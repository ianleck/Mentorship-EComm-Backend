    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "paypal-rest-sdk": "^1.8.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "5.22.3",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^1.1.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
